# Production Environment Configuration for YesCoin Web3

# BSCScan API Configuration
VITE_BSCSCAN_API_KEY=
VITE_BSCSCAN_API_URL=https://api.bscscan.com/api

# PancakeSwap API Configuration (DEX Aggregator)
VITE_PANCAKESWAP_API_URL=https://api.pancakeswap.info/api/v2

# 1inch API Configuration (Alternative DEX Aggregator)
VITE_ONEINCH_API_KEY=
VITE_ONEINCH_API_URL=https://api.1inch.dev

# CoinGecko API Configuration (for price data)
VITE_COINGECKO_API_KEY=
VITE_COINGECKO_API_URL=https://api.coingecko.com/api/v3

# BSC Network Configuration
VITE_BSC_RPC_URL=https://bsc-dataseed1.binance.org/
VITE_BSC_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545/
VITE_BSC_CHAIN_ID=56
VITE_BSC_TESTNET_CHAIN_ID=97

# Web3 Configuration
VITE_WALLETCONNECT_PROJECT_ID=
VITE_APP_NAME=YesCoin Web3
VITE_APP_DESCRIPTION=YesCoin Web3 DeFi Platform
VITE_APP_URL=https://yescoin-web3.vercel.app
VITE_APP_ICON=https://yescoin-web3.vercel.app/favicon.ico

# Rate Limiting Configuration (Production optimized)
VITE_API_RATE_LIMIT=10
VITE_API_CACHE_DURATION=600000
VITE_API_RETRY_ATTEMPTS=3
VITE_API_TIMEOUT=30000

# Performance Configuration
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_LOG_LEVEL=error